{
  "name": "marcosdly.dev",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "description": "Website for my stuff",
  "scripts": {
    "dev": "vite -m dev",
    "build": "tsc && vite build -m dev",
    "build:watch": "vite build -w -m dev",
    "build:prod": "npm run build:icons && tsc && vite build -m prod",
    "build:icons": "svg-icon build --source ./iconpack.json --target ./assets --name icons",
    "test": "vitest"
  },
  "keywords": [
    "website",
    "personal",
    "portfolio",
    "profile"
  ],
  "author": "Marcos Gabriel <dev.marcosdly@gmail.com>",
  "license": "MPL-2.0",
  "homepage": "https://marcosdly.dev",
  "devDependencies": {
    "@preact/preset-vite": "2.8.1",
    "@prettier/plugin-xml": "3.2.2",
    "@testing-library/preact": "3.2.3",
    "@types/node": "20.11.24",
    "@types/victor": "^1.1.5",
    "@vitest/browser": "1.3.1",
    "bootstrap-icons": "^1.11.3",
    "prettier": "3.2.5",
    "prettier-plugin-jsdoc": "1.3.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "sass": "1.70.0",
    "svg-icon": "0.8.2",
    "terser": "5.29.1",
    "typescript": "5.2.2",
    "vite": "5.1.1",
    "vite-raw-plugin": "1.0.2",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "preact": "10.19.6",
    "victor": "^1.1.0"
  }
}
